{
  "name": "sensbox-core-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "pretest": "node ./__tests__/setup/testServer.js",
    "test": "jest --runInBand --collect-coverage",
    "ci-local": "circleci local execute --config \".circleci/config.local.yml\" -v \"$(pwd)/coverage:/tmp/coverage\"",
    "dev": "nodemon -L . -i '**/logs/**' -i '**/__tests__/**' -i jest.**",
    "debug": "nodemon --inspect=0.0.0.0 -L . -i '**/logs/**' -i '**/__tests__/**' -i jest.**",
    "inspect": "node --inspect .",
    "start": "node .",
    "eslint": "eslint --debug --ext .js cloud",
    "seed": "for filename in ./seeds/*; do mongoimport --mode=upsert --uri ${MONGO_DSN} --file $filename;  done",
    "exportCollection": "dump() { mongoexport --uri ${MONGO_DSN} --collection $1 -o \"./seeds/$1.json\"  ; }; dump",
    "exportDB": "mongodump --uri ${MONGO_DSN} -o './backups/'$( date -Iseconds)",
    "importDB": "mongorestore --uri ${MONGO_DSN}"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "escape-html": "^1.0.3",
    "expo-server-sdk": "^3.1.0",
    "express": "^4.17.1",
    "hat": "0.0.3",
    "influx": "^5.4.1",
    "parse-dashboard": "^2.0.2",
    "parse-server": "^4.1.0",
    "saslprep": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^25.1.0",
    "nodemon": "^1.19.1",
    "parse": "^2.11.0"
  }
}
